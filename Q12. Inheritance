// Base class Animal
class Animal {
  // Method in the base class
  void makeSound() {
    print("Animal makes a sound");
  }
}

// Derived class Dog that inherits from Animal
class Dog extends Animal {
  // Override the makeSound method in the derived class
  @override
  void makeSound() {
    print("Dog barks");
  }
}

void main() {
  // Create an object of the Animal class
  Animal animal = Animal();
  animal.makeSound();  // Calls the makeSound method of Animal class

  // Create an object of the Dog class
  Dog dog = Dog();
  dog.makeSound();  // Calls the overridden makeSound method of Dog class
}